<settings>

    <setting propertyKey="@Properties.BackgroundColor" title="@Strings.BackgroundColorTitle">
        <settingConfig type="list">
            <listEntry value="0x000000">@Strings.ColorBlack</listEntry>
            <listEntry value="0x494848">@Strings.ColorDarkGray</listEntry>
            <listEntry value="0xBBBDBF">@Strings.ColorLightGray</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.ForegroundColor" title="@Strings.ForegroundColorTitle">
        <settingConfig type="list">
            <listEntry value="0x109AD7">@Strings.ColorBlue</listEntry>
            <listEntry value="0x00FF00">@Strings.ColorGreen</listEntry>
            <listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.CircleTimerColor" title="@Strings.CircleTimerColorTitle">
        <settingConfig type="list">
            <listEntry value="0x109AD7">@Strings.ColorBlue</listEntry>
            <listEntry value="0x00FF00">@Strings.ColorGreen</listEntry>
            <listEntry value="0xFFFFFF">@Strings.ColorWhite</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.CircleTimerArrows" title="@Strings.CircleTimerArrowsTitle">
        <settingConfig type="boolean" />
    </setting>

    <setting propertyKey="@Properties.TokensOrder" title="@Strings.TokensOrder">
        <settingConfig type="list">
            <listEntry value="0">@Strings.TokensOrderByIndex</listEntry>
            <listEntry value="1">@Strings.TokensOrderByName</listEntry>
        </settingConfig>
    </setting>


    <setting propertyKey="@Properties.TokenFormatFor6" title="@Strings.TokenFormatFor6">
        <settingConfig type="list">
            <listEntry value="0">@Strings.TokenFormatFor6_xxxxxx</listEntry>
            <listEntry value="1">@Strings.TokenFormatFor6_xxx_xxx</listEntry>
            <listEntry value="2">@Strings.TokenFormatFor6_xx_xx_xx</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.TokenFormatFor7" title="@Strings.TokenFormatFor7">
        <settingConfig type="list">
            <listEntry value="0">@Strings.TokenFormatFor7_yyyyyyy</listEntry>
            <listEntry value="1">@Strings.TokenFormatFor7_yyy_yyyy</listEntry>
            <listEntry value="2">@Strings.TokenFormatFor7_yyyy_yyy</listEntry>
            <listEntry value="3">@Strings.TokenFormatFor7_yyy_y_yyy</listEntry>
            <listEntry value="4">@Strings.TokenFormatFor7_yy_yyy_yy</listEntry>
        </settingConfig>
    </setting>

    <setting propertyKey="@Properties.TokenFormatFor8" title="@Strings.TokenFormatFor8">
        <settingConfig type="list">
            <listEntry value="0">@Strings.TokenFormatFor8_zzzzzzzz</listEntry>
            <listEntry value="1">@Strings.TokenFormatFor8_zzzz_zzzz</listEntry>
            <listEntry value="2">@Strings.TokenFormatFor8_zzz_zz_zzz</listEntry>
            <listEntry value="3">@Strings.TokenFormatFor8_zz_zzzz_zz</listEntry>
            <listEntry value="4">@Strings.TokenFormatFor8_zz_zz_zz_zz</listEntry>
        </settingConfig>
    </setting>


